<div id="content-header">
  <%= image_tag("/images/admission/admission_page_logo.jpg") %>
  <h1><%= t('admission') %></h1>
  <h3><%=  t('student_detail_choice') %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">
   
<% form_for(@student, :url => {:action=> 'import'}, :html=> {:multipart=>true}) do |s| %>
   <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>
    <span class="span-sub-heading"><%= t('upload_student_admission_excell') %></span>
        <hr class="sub-heading"></hr>
        <div class="label-field-pair">
          <label for="student_image_file"><%= t('upload_excell') %></label>
          <div class="text-input-bg"><%= s.file_field :excell %></div>

        </div>
      </div>
       <%= submit_tag "", :value => " #{t('upload_excell')}", :class => "submit_button" %>
  <% end %><br />
	<span class="span-sub-heading"><%= t('Fill_student_admission_detail') %></span>
        <hr class="sub-heading"></hr>
	<div id="user_options">	
	<div class="button-box">
      <%= link_to "<div class ='button-label'><p>#{t('admission')}</p></div>", {:controller => "student", :action => "admission1"},
        :class => "option_buttons", :id => "admission_button", :title => "#{t('admission_link_title')}"%>
    </div>
    </div>
</div>
